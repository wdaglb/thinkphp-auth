{
    "name": "ke/thinkphp-auth",
    "description": "the thinkphp auth",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "ke/thinkphp-migration": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ke\\auth\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ke\\auth\\tests\\": "tests/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    },
    "require-dev": {
        "topthink/framework": "5.1.*",
        "phpunit/phpunit": "^9.1"
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist"
    }
}
